


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>RESULTS</title>

    <link rel="stylesheet" href="~/Scripts/bootstrap.min.css">
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
    <script src="@Url.Content("https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js")"></script>
    <script src="@Url.Content("https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js")"></script>
    <style>
        .footer {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            height: 42px;
            background-color: #333;
            color: #f2f2f2;
            text-align: center;
            opacity: 5;
        }
        .scrolltop {
            display: none;
            width: 100%;
            color: #f1f1f1;
            background-color: #f1f1f1;
            margin: 0 auto;
            position: fixed;
            bottom: 20px;
            right: 10px;
        }



        .scroll {
            position: absolute;
            right: 10px;
            left: -10px;
            bottom: 20px;
            background: #b2b2b2;
            background: rgba(178,178,178,0.7);
            padding: 20px;
            text-align: center;
            margin: 0 0 0 0;
            cursor: pointer;
            transition: 0.5s;
            -moz-transition: 0.5s;
            -webkit-transition: 0.5s;
            -o-transition: 0.5s;
        }

            .scroll:hover {
                background: rgba(178,178,178,1.0);
                transition: 0.5s;
                -moz-transition: 0.5s;
                -webkit-transition: 0.5s;
                -o-transition: 0.5s;
            }

                .scroll:hover .fa {
                    padding-top: -10px;
                }

            .scroll .fa {
                font-size: 30px;
                margin-top: -5px;
                margin-left: 1px;
                transition: 0.5s;
                -moz-transition: 0.5s;
                -webkit-transition: 0.5s;
                -o-transition: 0.5s;
            }

            .scroll icon {
                margin-right: 0px;
            }

        .scrolltop {
            margin: auto;
            width: 1%;
            padding: 10px;
        }

        i {
            position: center;
            border: solid whitesmoke;
            border-width: 0 5px 5px 0;
            display: inline-block;
            color: red;
        }

        .up {
            transform: rotate(-135deg);
            -webkit-transform: rotate(-135deg);
            padding: 3px;
            margin: 0px,1px,1px,0px;
            color: red;
        }
        #section2{height:760px;

        }
        #section1 {
            height: 400px;
        }
        #section3 {
            height: 0px;
        }

        @@media (min-width:801px) {
            #zvezda {
                margin: auto;
                width: 800px;
                padding: 10px;
                display: flex;
                justify-content: center;
            }
        }


    </style>
</head>
<body style="background-image:url('/img/melissa-askew-15335-unsplash.jpg');background-size:100%;background-size:cover;opacity:0.8;">
    <div class='thetop'></div>
    <div id="section1" class="container-fluid" style="background-image:url('/img/melissa-askew-15335-unsplash.jpg');background-size:100%;background-size:cover;opacity:1;">
        <div class='thetop'></div><br /><br /><br />
        <h1 style="color:yellow;font-size:55px;text-align:center;left:400px;">RESULTS:</h1>

        <div id="zvezda">
            <span style="font-size:1500%;color:yellow;">&bigstar;</span>
            <span style="font-size:1500%;color:yellow;">&bigstar;</span>
            <span style="font-size:1500%;color:yellow;">&bigstar;</span>
        </div>
    </div>



    <div id="section2">

        @RenderBody()


    </div>
    <div id="section3" class="container-fluid" style="background-image:url('/img/melissa-askew-15335-unsplash.jpg');background-size:100%;background-size:cover;opacity:3;">



        <div id="zvezda">
            <span style="font-size:1500%;color:yellow;">&bigstar;</span>
            <span style="font-size:1500%;color:yellow;">&bigstar;</span>
            <span style="font-size:1500%;color:yellow;">&bigstar;</span>
        </div>
    </div>
    <div class='scrolltop' style="display: none;">
        <div class='scroll icon' style="width:3px;height:3px;margin-right:2px;margin-left:2px;background-color:rgb(167, 11, 11);"><i class="up" style="padding:5px;margin:0px;position:absolute;right:5px;width:4px;"></i></div>
    </div>

    <div class="footer">
        <p style="margin: 9px;">
            Copyright &copy; DejanS 2019
        </p>
    </div>

    <script>
        var prevScrollpos = window.pageYOffset;
        window.onscroll = function () {
            var currentScrollPos = window.pageYOffset;
            if (prevScrollpos > currentScrollPos) {
                document.getElementById("navbar").style.top = "0";
            } else {
                document.getElementById("navbar").style.top = "-50px";
            }
            prevScrollpos = currentScrollPos;
        }
    </script>

    <script>
        $(window).scroll(function () {
            if ($(this).scrollTop() > 50) {
                $('.scrolltop:hidden').stop(true, true).fadeIn();
            } else {
                $('.scrolltop').stop(true, true).fadeOut();
            }
        });
        $(function () { $(".scroll").click(function () { $("html,body").animate({ scrollTop: $(".thetop").offset().top }, "1000"); return false }) })

    </script>


</body>
</html>
